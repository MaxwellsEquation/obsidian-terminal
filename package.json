{
	"name": "obsidian-terminal",
	"displayName": "Obsidian Terminal",
	"version": "2.1.0",
	"author": "polyipseity",
	"license": "MIT",
	"description": "Open terminals in Obsidian directly.",
	"keywords": [
		"obsidian",
		"plugin",
		"terminal"
	],
	"private": true,
	"main": "main.js",
	"scripts": {
		"dev": "node build/esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node build/esbuild.config.mjs production",
		"version": "node build/version-bump.mjs && git add manifest.json versions.json",
		"postversion": "node build/version-commit.mjs"
	},
	"dependencies": {
		"ffi-napi": "4.0.3",
		"i18next": "22.4.6",
		"tmp": "0.2.1",
		"xterm": "5.1.0",
		"xterm-addon-fit": "0.7.0",
		"xterm-addon-search": "0.11.0",
		"xterm-addon-web-links": "0.8.0"
	},
	"optionalDependencies": {
		"win32-api": "20.2.0",
		"win32-def": "20.0.0"
	},
	"devDependencies": {
		"@types/ffi-napi": "^4.0.7",
		"@types/node": "^16.18.10",
		"@types/tmp": "^0.2.3",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"obsidian": {
		"id": "terminal",
		"name": "Terminal",
		"minAppVersion": "1.1.9",
		"authorUrl": "",
		"fundingUrl": {},
		"isDesktopOnly": true
	}
}